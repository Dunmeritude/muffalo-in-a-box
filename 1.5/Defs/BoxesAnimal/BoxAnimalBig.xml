                    <!-- Bison-->
                    <li>
                        <key>
                            <pawnKindDef>Bison</pawnKindDef>
                            <genderOfPawn>Male</genderOfPawn>
                        </key>
                        <value>StasisBox_Bison_Male</value>
                    </li>
                    <li>
                        <key>
                            <pawnKindDef>Bison</pawnKindDef>
                            <genderOfPawn>Female</genderOfPawn>
                        </key>
                        <value>StasisBox_Bison_Female</value>
                    </li>
