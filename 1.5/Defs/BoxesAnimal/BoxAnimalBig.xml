<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef ParentName="StatisBoxBase">
        <defName>StasisBox_EmptyAnimalBig</defName>
        <label>large stasis box</label>
        <description>A legally distinct non-spheroid stasis box ready to hold a large animal. A good way to preserve your herd when feeding them is not an easily available option, but you have storage space and energy to spare.</description>
        <graphicData>
          <texPath>StasisBox/AnimalBoxes/BigAnimalBoxes/Empty</texPath>
          <graphicClass>Graphic_StackCount</graphicClass>
          <drawSize>(1.5,1.5)</drawSize>
        </graphicData>
        <costList>
            <Kibble>40</Kibble>
            <Steel>40</Steel>
            <Plasteel>20</Plasteel>
            <ComponentIndustrial>5</ComponentIndustrial>
        </costList>
        <recipeMaker>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <recipeUsers>
              <li>StasisBox_PackingBench</li>
            </recipeUsers>
            <researchPrerequisite>Machining</researchPrerequisite>
            <workAmount>800</workAmount>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Smith</soundWorking>
        </recipeMaker>
        <comps>
            <li Class="StasisBox.CompProperties_BoxPawns">
                <targetingParams>
                    <canTargetHumans>false</canTargetHumans>
                    <canTargetMechs>false</canTargetMechs>
                    <canTargetBuildings>false</canTargetBuildings>
                    <canTargetMutants>false</canTargetMutants>
                    <canTargetBloodfeeders>false</canTargetBloodfeeders>
                </targetingParams>
