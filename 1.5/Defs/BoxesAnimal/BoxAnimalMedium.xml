<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef ParentName="StatisBoxBase">
        <defName>StasisBox_EmptyAnimalMedium</defName>
        <label>medium stasis box</label>
        <description>A legally distinct non-spheroid stasis box ready to hold a medium animal. A good way to preserve your herd when feeding them is not an easily available option, but you have storage space and energy to spare.</description>
        <graphicData>
          <texPath>StasisBox/AnimalBoxes/MediumAnimalBoxes/Empty</texPath>
          <graphicClass>Graphic_StackCount</graphicClass>
          <drawSize>(1.5,1.5)</drawSize>
        </graphicData>
        <costList>
            <Kibble>20</Kibble>
            <Steel>20</Steel>
            <Plasteel>20</Plasteel>
            <ComponentIndustrial>1</ComponentIndustrial>
        </costList>
        <recipeMaker>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <recipeUsers>
              <li>StasisBox_PackingBench</li>
            </recipeUsers>
            <researchPrerequisite>Machining</researchPrerequisite>
            <workAmount>450</workAmount>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Smith</soundWorking>
        </recipeMaker>
        <comps>
            <li Class="StasisBox.CompProperties_BoxPawns">
                <targetingParams>
                    <canTargetHumans>false</canTargetHumans>
                    <canTargetMechs>false</canTargetMechs>
                    <canTargetBuildings>false</canTargetBuildings>
                    <canTargetMutants>false</canTargetMutants>
                    <canTargetBloodfeeders>false</canTargetBloodfeeders>
                </targetingParams>
