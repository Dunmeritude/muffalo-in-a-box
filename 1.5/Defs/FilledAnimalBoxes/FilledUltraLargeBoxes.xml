<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef ParentName="FilledAnimalCategory" Name="FilledveryLargeAnimalCategory" Abstract="true">
      <description>A legally distinct non-spheroid stasis box containing a very large animal. A good way to preserve your herd when feeding them is not an easily available option, but you have storage space and energy to spare.</description>
      <graphicData>
          <texPath>StasisBox/AnimalBoxes/UltraLargeAnimalBoxes/Filled</texPath>
          <graphicClass>Graphic_StackCount</graphicClass>
          <drawSize>(1.5,1.5)</drawSize>
        </graphicData>
    </ThingDef>

    <ThingDef ParentName="FilledveryLargeAnimalCategory">
        <defName>StasisBox_Thrumbo_Female</defName>
        <label>very large stasis box (Thrumbo)</label>
        <statBases>
            <MarketValue>4000</MarketValue>
        </statBases>
        <comps>
            <li Class="StasisBox.CompProperties_BoxPawns">
                <emptyBox>StasisBox_EmptyAnimalUltraLarge</emptyBox>
                <openable>true</openable>
                <pawnKindToSpawnWhenOpened>Thrumbo</pawnKindToSpawnWhenOpened>
                <genderOfPawn>Female</genderOfPawn>
            </li>
        </comps>
    </ThingDef>
    <ThingDef ParentName="FilledveryLargeAnimalCategory">
        <defName>StasisBox_Thrumbo_Male</defName>
        <label>very large stasis box (Thrumbo)</label>
        <statBases>
            <MarketValue>4000</MarketValue>
        </statBases>
        <comps>
            <li Class="StasisBox.CompProperties_BoxPawns">
                <emptyBox>StasisBox_EmptyAnimalUltraLarge</emptyBox>
                <openable>true</openable>
                <pawnKindToSpawnWhenOpened>Thrumbo</pawnKindToSpawnWhenOpened>
                <genderOfPawn>Male</genderOfPawn>
            </li>
        </comps>
    </ThingDef>

</Defs>